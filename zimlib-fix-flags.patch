diff --git a/meson.build b/meson.build
index 2d38684..42e5253 100644
--- a/meson.build
+++ b/meson.build
@@ -1,7 +1,7 @@
 project('libzim', ['c', 'cpp'],
   version : '4.0.6',
   license : 'GPL2',
